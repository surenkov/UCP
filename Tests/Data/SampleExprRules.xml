<?xml version="1.0" encoding="utf-8" ?>
<grammar xmlns="http://savva.moe/compiler/grammar.xsd">
  <start rule="E"/>
  <rule term="E" production="T $+ E"/>
  <rule term="E" production="T"/>
  <rule term="T" production="F $* T"/>
  <rule term="T" production="F"/>
  <rule term="F" production="$( E $)"/>
  <rule term="F" production="$a"/>
</grammar>
