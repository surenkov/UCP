<?xml version="1.0" encoding="utf-8" ?>
<rules xmlns="http://savva.moe/compiler/rules.xsd">
  <start term="P" production="S" />

  <rule term="S" production="S $+ M" />
  <rule term="S" production="M" />

  <rule term="M" production="M $* T" />
  <rule term="M" production="T"/>

  <rule term="T" production="$integral" />
</rules>
